;; Envos site init file(in-package "INTERLISP");; load in the standard "released" one(load "{DSK}/usr/local/lde/lisplibrary/init.maiko");; allow internal/library(appendtovar lispusersdirectories 	"{DSK}/usr/local/lde/lispcore/internal/library/");; add sources(appendtovar directories 	"{DSK}/usr/local/lde/lispcore/sources/"	"{DSK}/usr/local/lde/lispcore/library/"	"{DSK}/usr/local/lde/lispcore/internal/library/"	"{DSK}/usr/local/lde/lispcore/lispusers/");; system hash files(cl:when (getd 'xcl::add-where-is-database) 	 (setq xcl::*where-is-cash-files*)	 (xcl::add-where-is-database	  "{DSK}/usr/local/lde/lispcore/sources/system.hash"));; allow init files in a few other places(appendtovar usergreetfiles	     ("{DSK}/" user "/lisp/init." com)	     ("{DSK}/" user "/lisp/init.lisp")	     ("{DSK}/" user "/medley/init." com)	     ("{DSK}/" user "/medley/init.lisp")	     ("{DSK}/" user "/init." com)	     ("{DSK}/" user "/init.lisp") );; perhaps change these if there is a new clearinghouse;; and everyone is in it(setq ch.default.domain "MV")(setq ch.default.organization "envos")(setq authentication.net.hint	(setq ch.net.hint (list 138)))(setq defaultregistry 'envos);; Copyright info(setq copyrightflg 'default)(setq copyrightowners '((Venue "Venue")))(setq defaultcopyrightowner 'Venue)(setq copyrightreserved t);; Mail(setq nsmail.net.hint (list 138));; Printing(setq printservice 11.0)(setq defaultprintinghost '(|Twister:MV:envos|))(setq defaultfaxhost "UniversalFAX:OSBU NORTH:Xerox")(addtovar faxaddresses	(lucid/fax 94153298480)	(franz/fax 94155488253)	(ssl/fax 94154944334)	(europarc/fax 901144223341510)	(microlytics/fax 97162483868));; Chat hosts(addtovar networkostypes	(gslvax . vms)	(gslvax10 . vms)	(xsvax . vms)	(arisia . unix)	(|ARISIA:PARC:XEROX| . unix)	(pooh . unix)	(herbrand . unix)	(palain . unix));; Site name(setq XCL:*SHORT-SITE-NAME* "Venue")(setq XCL:*LONG-SITE-NAME* "Venue")