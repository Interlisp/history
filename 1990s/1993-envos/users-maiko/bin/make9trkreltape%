# (invoke C shell)echo ""echo ""echo "You should already have a tape mounted in your cartridge drive,"echo "and you should be in the particular root directory,"echo "e.g. /python/maiko3-1.1.beta:"echo ""echo "Please verify that you are in the correct directory."echo "You are currently in:"echo ""pwd echo ""echo "If incorrect, type ^C to exit."echo "If   correct, type ^D"echo ""catecho "OK, making tape (operation can still be aborted with ^C)..."tar chfb /dev/rst0 126 .echo ""echo "Finally remove the tape and turn the write protect to SAFE"echo ""