#!/bin/cshif($1 == "") then	echo "Usage:: dosput f1 ... fn"	exitendifforeach file ($*)	switch ($file)	case *.s:	case *.c:		set dir='src'		breaksw	case *.il:		set dir='src'		breaksw	case *.h:		set dir='inc'		breaksw	case *.lex:		set dir='src'		breaksw	default:		set dir='bin'	endswrsh pcai "/usr/bin/doserase c:/maiko/src/$file"rsh pcai "/usr/bin/dosput /python1/users/maiko/DOS/$dir/$file c:/maiko/src/$file"end