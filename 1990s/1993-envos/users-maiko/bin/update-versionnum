#update:  bin|src|inc [s]switch ($2)case s*:	set dir1='stable'	breakswcase w*:	set dir1='working'	breakswdefault:	set dir1='working'	breakswendswset current=`pwd`switch ($1)case bin:	set dir2='bin'	cd /users/maiko/$dir1/bin/	set files=`ls *`	cd ~/maiko/bin/	breakswcase inc:	set dir2='inc'	cd /users/maiko/$dir1/inc/	set files=`ls *.h`	cd ~/maiko/inc/	breakswcase src:	set dir2='src'	cd /users/maiko/$dir1/src/	set files=`ls *.c *.il`	cd ~/maiko/src/	echo ".o files are not updated. They will be updated by makefile."	breakswcase all:	update bin	update inc	update src	exitdefault:	echo "(update error) Argument 1 must be bin, inc, or src"	exitendswforeach file ($files)	if ($file =~ *.sysout ) then		echo "($file not compared)"	else		/usr/sccs/get -e -r2 /users/maiko/SCCS/s.$file		/usr/sccs/delta -y"Version up to 2.1" /users/maiko/SCCS/s.$file	endifendcd $currentecho ""