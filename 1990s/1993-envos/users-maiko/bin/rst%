#reset: file1 ... fileNif($1 == "") then	echo "Usage:: rst f1 ... fn"	exitendifforeach file ($*)	echo $file	if (-w $file) then		/usr/sccs/unget /users/maiko/SCCS/s.$file		switch ($file)		case *.c:		case *.il:			set dir='src'			breaksw		case *.h:			set dir='inc'			breaksw		default:			set dir='bin'		endsw		ln /users/maiko/working/$dir/$file	else		echo "(reset error) Not writable file!!"	endifend