#check out: file1 ... fileNif($1 == "") then	echo "Usage:: co f1 ... fn"	exitendifforeach file ($*)	echo $file	echo "p.$file"	set already=`/users/maiko/bin/checkp | grep  "p\.$file"'$'`	if ($#already >= 1) then		echo "(co error) Duplicate CheckOut."		exit	endif	/bin/rm -f $file#	switch($file)#	case *.c:#		set ofile=`echo $file | sed 's/.c/.o/'`#		if(-e $ofile) then#			rm -f $ofile#		endif#	endsw		/usr/sccs/get -e /users/maiko/SCCS/s.$fileend