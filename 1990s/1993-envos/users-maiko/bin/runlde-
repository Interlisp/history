# First comment line required (invokes CSH)# =============================================================================# Relies on shell variable MYSYSOUT (which can be set in your .login or .cshrc)# to start up lisp.  The cases below are for the following machines:##	trouser		1300283a#	cobra		13001e50#	viper		13001e98#	mamba		13001da3#	squonk		11002517#	garter		17003b46#	timber		1700319b#	gopher		17003016#	water		22000950#	naga		220009f9## Switch statement would be nicer to condition on hostname instead of hostid,# but I built this file automatically from output of keymaker, which doesn't# know about hostnames....# =============================================================================if ($?MYSYSOUT != '1') then    if ($?LDESRCESYSOUT == '1') then	setenv MYSYSOUT $LDESRCESYSOUT    else    if ($?LDESOURCESYSOUT == '1') then	setenv MYSYSOUT $LDESOURCESYSOUT	endif    endifendifswitch ("`hostname`")    case 'trouser':	$LDE $MYSYSOUT -k 'ac9a0794 b313abae 73421322'	breaksw    case 'cobra':	$LDE $MYSYSOUT -k '10bfab0c 477ef55e a5ad1b12'	breaksw    case 'viper':	$LDE $MYSYSOUT -k '4f470760 51860f70 80453d60'	breaksw    case 'mamba':	$LDE $MYSYSOUT -k '12abb456  d6ab9f7 6740c0cb'	breaksw    case 'squonk':	$LDE $MYSYSOUT -k 'b3217f6f 8cd14878 a7d84260'	breaksw    case 'garter':	$LDE $MYSYSOUT -k '46e27958 4d23421c c4632dc0'	breaksw    case 'timber':	$LDE $MYSYSOUT -k '99e8bfc6 92299f45 9199a409'	breaksw    case 'gopher':	$LDE $MYSYSOUT -k '70c5a8d8 7b0498cc 45e35500'	breaksw    case 'water':	$LDE $MYSYSOUT -k '141a220c 72db6b5e 272d1a12'	breaksw    case 'naga':	$LDE $MYSYSOUT -k '31a7f54f f7afb41b 10ee906b'	breaksw    case 'glossy':	$LDE $MYSYSOUT -k 'a6d332cf 9d23a1e8 301225a0'	breaksw    default:	echo "Sorry, host '`hostname`' not in this shell script"endsw