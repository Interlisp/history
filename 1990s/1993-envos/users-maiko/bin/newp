#newp [bin|src|inc|]set current=`pwd`set dir1="working"switch ($1)case bin:	set dir2='bin'	cd /users/maiko/$dir1/bin/	set files=`ls *`	cd ~/maiko/bin/	breakswcase inc:	set dir2='inc'	cd /users/maiko/$dir1/inc/	set files=`ls *.h`	cd ~/maiko/inc/	breakswcase src:	set dir2='src'	cd /users/maiko/$dir1/src/	set files=`ls *.c *.il *.lex`	cd ~/maiko/src/	breakswdefault:	newp bin	newp src	newp inc	exitendswecho "comparing $dir2 to $dir1"foreach file ($files)	if(! -e $file) then		echo "new file[$file]"	else if ($file =~ *.sysout) then		echo "(sysout)"	else		set x=`cmp $file /users/maiko/$dir1/$dir2/$file`		if($#x != 0) then			echo  "different[$file]"		else			echo -n '.' 		endif	endifendcd $currentecho ""