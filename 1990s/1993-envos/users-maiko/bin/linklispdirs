# ------- first comment line invokes c shell## (script will create xxxx/lispxxxx)## Assumes srce, dest directories given on command line ($1, $2).#if ($#argv < 2) then	echo ""	echo "usage: linklispdirs srcedir destdir"	echo "       dirs should be root dirs, e.g.:  /python/maiko4-1.1.beta"	echo "       dest subdirs will be created automatically."	echo ""	exitendifsetenv LISPSRCEDIR $1setenv LISPDESTDIR $2alias chown /etc/chownforeach dir (lisplibrary lispsysouts)	mkdir     $LISPDESTDIR/$dir	chown bin $LISPDESTDIR/$dir	echo      $LISPDESTDIR/$dir	cd        $LISPSRCEDIR/$dir	foreach file (*)		ln $file    $LISPDESTDIR/$dir/$file		chown bin   $LISPDESTDIR/$dir/$file		echo "    " $LISPDESTDIR/$dir/$file		end	end