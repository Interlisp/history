ref-21                    Xerox Quintus Prolog Reference Manual                        Chapter 21: Built-in Operators:-op( 1200, xfx, [ :-, --> ]):-op( 1200,  fx, [ :-, ?- ]):-op( 1150,  fx, [ mode, public, dynamic, multifile,                                    meta_predicate ]):-op( 1100, xfy, [ ; ]):-op( 1050, xfy, [ -> ]):-op( 1000, xfy, [ ',' ]):-op(  900,  fy, [ \+, spy, nospy ]):-op(  700, xfx, [ =, is, =.., ==, \==, @<, @>, @=<, @>=,                                    =:=, =\=, <, >, =<, >= ]):-op(  600, xfy, [:]):-op(  500, yfx, [ +, -, \/, /\ ]):-op(  500,  fx, [ +, - ]):-op(  400, yfx, [ /, //, *, <<, >> ]):-op(  300, xfx, [ mod ]):-op(  200, xfy, [ ^ ])