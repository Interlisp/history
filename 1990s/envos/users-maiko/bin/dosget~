#!/bin/cshunalias mvif($1 == "") then	echo "Usage:: dosget f1 ... fn"	exitendifforeach file ($*)	switch ($file)	case *.s:	case *.c:		set dir='src'		breaksw	case *.il:		set dir='src'		breaksw	case *.h:		set dir='inc'		breaksw	case *.lex:		set dir='src'		breaksw	default:		set dir='bin'	endswmv /python1/users/maiko/DOS/$dir/$file /python1/users/maiko/DOS/$dir/$file.tmprsh pcai "/usr/bin/dosget c:/maiko/src/$file /python1/users/maiko/DOS/$dir/$file"end