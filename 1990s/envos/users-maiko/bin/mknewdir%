#mknewdir: make new directories and setup. Do on home directoryecho "This command makes some directories for Maiko working on your home directory."echo -n 'Are you sure ? (y/n)'if ($< == n) then	exitendifset current=`pwd`cdmkdir maikochdir maikomkdir binmkdir srcmkdir incecho "Start linking on bin files ..."cd binset files=`ls /users/maiko/working/bin/`foreach file ($files)	ln /users/maiko/working/bin/$fileendecho "Start linking on src files ..."cd ../srcset files=`ls /users/maiko/working/src/`foreach file ($files)	ln /users/maiko/working/src/$fileendecho "Start linking on inc files ..."cd ../incset files=`ls /users/maiko/working/inc/`foreach file ($files)	ln /users/maiko/working/inc/$fileendcd $current