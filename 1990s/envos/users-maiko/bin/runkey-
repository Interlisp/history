# First comment line required (invokes CSH)# =============================================================================# This script provides keys for various hosts in Envos.  Feel free to add# your machine and hostkey below.## 4/110:	water		22000950	naga		220009f9# 3/260:	trouser		1300283a	cobra		13001e50#		viper		13001e98	mamba		13001da3# 3/060:	timber		1700319b	gopher		17003016# 3/060: os4.0	glossy		17009127	corn		17008836#		garter		17003b46# 3/140:	squonk		11002517## =============================================================================switch ("`hostname`")    case 'trouser':	echo 'ac9a0794 b313abae 73421322'	breaksw    case 'cobra':	echo '10bfab0c 477ef55e a5ad1b12'	breaksw    case 'viper':	echo '4f470760 51860f70 80453d60'	breaksw    case 'mamba':	echo '12abb456  d6ab9f7 6740c0cb'	breaksw    case 'squonk':	echo 'b3217f6f 8cd14878 a7d84260'	breaksw    case 'garter':	echo '46e27958 4d23421c c4632dc0'	breaksw    case 'timber':	echo '99e8bfc6 92299f45 9199a409'	breaksw    case 'gopher':	echo '70c5a8d8 7b0498cc 45e35500'	breaksw    case 'water':	echo '141a220c 72db6b5e 272d1a12'	breaksw    case 'naga':	echo '31a7f54f f7afb41b 10ee906b'	breaksw    case 'glossy':	echo 'a6d332cf 9d23a1e8 301225a0'	breaksw    case 'corn':	echo ' 3347b98  8f5f3ac ada43510'    default:	echo "host-unknown"endsw