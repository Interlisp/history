#cfdiff:  bin|src|inc|all [w]#compare files on /python/maiko.freeze/stable/bin(src|inc) with /users/maiko/stable(working)/bin(src|inc)if($1 == "") then	echo "Usage:: cfdiff bin|src|inc|all [w]"	exitendifswitch ($2)case s*:	set dir1='stable'	breakswcase w*:	set dir1='working'	breakswdefault:	set dir1='stable'	breakswendswset current=`pwd`switch ($1)case bin:	set dir2='bin'	cd /users/maiko/$dir1/bin/	set files=`ls *`	cd /python/maiko.freeze/stable/bin/	echo "compare /python/maiko.freeze/stable/bin with /users/maiko/$dir1/bin"	breakswcase inc:	set dir2='inc'	cd /users/maiko/$dir1/inc/	set files=`ls *.h`	cd /python/maiko.freeze/stable/inc/	echo "compare /python/maiko.freeze/stable/inc with /users/maiko/$dir1/inc"	breakswcase src:	set dir2='src'	cd /users/maiko/$dir1/src/	set files=`ls *.c *.il`	cd /python/maiko.freeze/stable/src/	echo "compare /python/maiko.freeze/stable/src with /users/maiko/$dir1/src"	breakswcase all:	cfdiff bin $dir1	cfdiff inc $dir1	cfdiff src $dir1	exitdefault:	echo "(cfdiff error) Argument 1 must be bin, inc, src or all"	exitendswforeach file ($files)	if(! -e $file) then		echo "New file[$file]"	else if ($file =~ *.sysout ) then		echo "($file is not compared)"	else		set x=`cmp $file /users/maiko/$dir1/$dir2/$file`		if($#x != 0) then			echo "$file is different"		endif	endifendcd $currentecho ""