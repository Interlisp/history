#update:  bin|src|inc [s]switch ($2)case s*:	set dir1='stable'	breakswcase w*:	set dir1='working'	breakswdefault:	set dir1='working'	breakswendswset current=`pwd`switch ($1)case bin:	set dir2='bin'	cd /users/maiko/$dir1/bin/	set files=`ls *`	cd /python2/tmp/maiko/bin/	breakswcase inc:	set dir2='inc'	cd /users/maiko/$dir1/inc/	set files=`ls *.h`	cd /python2/tmp/maiko/inc/	breakswcase src:	set dir2='src'	cd /users/maiko/$dir1/src/	set files=`ls *.s  *.c *.il *.lex`	cd /python2/tmp/maiko/src/	breakswcase scripts:	set dir2='scripts'	cd /users/maiko/$dir1/scripts/	set files=`ls *`	cd /python2/tmp/maiko/scripts/	breakswcase all:	update bin $2	update inc $2	update src $2	update scripts $2	exitdefault:	echo "(update error) Argument 1 must be bin, inc, or src"	exitendswecho "updating $dir2 from $dir1"foreach file ($files)	if(! -e $file) then		echo "You get new file[$file]"		cp /users/maiko/$dir1/$dir2/$file .	else if ($file =~ *.sysout ) then		echo "($file not compared)"	else		set x=`cmp $file /users/maiko/$dir1/$dir2/$file`		if($#x != 0) then			echo $file			if(-w $file) then				echo "Saving your writable version to $file-"				cp $file $file-			else			endif			/bin/rm -f $file			cp /users/maiko/$dir1/$dir2/$file .		endif	endifendcd $currentecho ""