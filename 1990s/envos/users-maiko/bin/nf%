#newfile: file1 ... fileNif($1 == "") then	echo "Usage:: nf f1 ... fn"	exitendifforeach file ($*)	echo $file	if (-e /users/maiko/SCCS/s.$file) then		echo "(newfile error) The file has already existed."	else		switch ($file)		case *.c:			cat /users/maiko/bin/sccsid.c $file > temp.$file			mv temp.$file $file			set dir='src'			breaksw		case *.il:			cat /users/maiko/bin/sccsid.h $file > temp.$file			mv temp.$file $file			set dir='src'			breaksw		case *.s:			cat /users/maiko/bin/sccsid.s $file > temp.$file			mv temp.$file $file			set dir='src'			breaksw		case *.h:			cat /users/maiko/bin/sccsid.h $file > temp.$file			mv temp.$file $file			set dir='inc'			breaksw		case *.lex:			cat /users/maiko/bin/sccsid.lex $file > temp.$file			mv temp.$file $file			set dir='src'			breaksw		default:			set dir='bin'		endsw		/usr/sccs/admin -i$file /users/maiko/SCCS/s.$file		rm $file		set current=`pwd`		cd /users/maiko/working/$dir/		/usr/sccs/get /users/maiko/SCCS/s.$file		cd $current		ln /users/maiko/working/$dir/$file		echo "New file is saved to SCCS and working directory."		echo "You must rewrite makefile by yourself."	endifend