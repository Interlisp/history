#check in: file1 ... fileNif($1 == "") then	echo "Usage:: ci f1 ... fn"	exitendifforeach file ($*)	set already=`/users/maiko/bin/checkp | grep $file`	if ($#already == 0) then		exit	endif	set flag=0	switch ($file)	case *.s:	case *.c:#		set ofile=`echo $file | sed 's/.c/.o/'`#		set flag=1		set dir='src'		breaksw	case *.il:		set dir='src'		breaksw	case *.h:		set dir='inc'		breaksw	case *.lex:		set dir='src'		breaksw	default:		set dir='bin'	endsw	echo $file	/usr/sccs/delta /users/maiko/SCCS/s.$file	set cdir=`pwd`	cd /users/maiko/working/$dir/	/bin/rm -f $file	sccs get /users/maiko/SCCS/s.$file	cd $cdir	ln /users/maiko/working/$dir/$file#	sccs get /users/maiko/SCCS/s.$file#	if($flag == 1) then#		if(-e /users/maiko/working/$dir/$ofile) then#			rm /users/maiko/working/$dir/$ofile#		endif#		cp $file /users/maiko/working/$dir/$ofile#		ln /users/maiko/working/$dir/$ofile#	endifend