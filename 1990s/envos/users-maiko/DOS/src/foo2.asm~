.MODEL FLATNAME foo2CODE32 SEGMENT READONLY PUBLIC USE32ASSUME DS:DATA		PUBLIC Mouse		PUBLIC PostAmblerPostAmbler proc near		popad	; Restore all the registers		retPostAmbler endpMouse proc near		pushad	; Save all the registers		push DWORD PTR PostAmbler		push 42 ; Push the arg for the mouse handler		jmp Mhandler	; We'll never return from here.Mouse endpCODE32 ENDSEND