.model small.stack 200h.dataparmblok	DD 0(0)envseg	DW	0	; environment (0 = copy mine)cmdip	DWcmdcs	DWfcb1	DW	2(0)fcb2	DW	2(0)stk	DW	2(0)csip	DW	2(0)errstg	DB	'ERROR:  Couldn''t free excess storage.',13,10,'$'emulpath DB	'emul.exe', 13, 10, '$'.codeprints	macro	text	mov	dx, OFFSET text	mov	ah,9	int	21h	endmPUBLIC	mainmain	PROC	NEAR	mov	ax,@data	mov	ds,ax	mov	cmdcs, cs	mov	cmdip, 80h	mov	ax, seg endall	add	ax,1	mov	es,ax	mov	ax,4900h	int	21h	jnc	freeok	prints	errstg;	ret	mov ax,0	int 21hfreeok:	 prints emulpathmov ax,0	int 21h	mov	ax,4b01h	; load-don't-start	mov	dx, OFFSET emulpath	mov	ax, ds	mov	es, ax	mov	bx, OFFSET parmblok	retmain	endp;	compare [ds]di & es:[si] for cx chars.  If equal, return zero flag, else not.strcmp	proc	near	cld	push	ds	push di	push	es	push si	push cx	repe	cmpsb	pop	cx	pop	si	pop	es	pop	di	pop	ds	ret	nearstrcmp	endpendall:END