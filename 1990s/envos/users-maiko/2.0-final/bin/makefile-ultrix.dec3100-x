# Options for Ultrix, DECStation 3100, and X-WindowCC = gccXFILES = $(OBJECTDIR)XClose.o \	$(OBJECTDIR)Cursor.o \	$(OBJECTDIR)XWindow.o \	$(OBJECTDIR)DoRing.o \	$(OBJECTDIR)DoScroll.o \	$(OBJECTDIR)XEvent.o \	$(OBJECTDIR)XGravity.o \	$(OBJECTDIR)XInit.o \	$(OBJECTDIR)InitXevent.o \	$(OBJECTDIR)XKbdMouse.o \	$(OBJECTDIR)LispWindow.o \        $(OBJECTDIR)LispXbitblt.o \	$(OBJECTDIR)XKeyboard.o \	$(OBJECTDIR)MakeXicon.o \	$(OBJECTDIR)OpenDisplay.o \	$(OBJECTDIR)ReadXoption.o \	$(OBJECTDIR)XReconfig.o \	$(OBJECTDIR)XScroll.o \	$(OBJECTDIR)Scrollbar.o \	$(OBJECTDIR)XCursor.o \	$(OBJECTDIR)XMouse.o \	$(OBJECTDIR)Subwindows.o \	$(OBJECTDIR)VideoColor.o \	$(OBJECTDIR)XWindowMgr.o  BYTESWAPFILES = $(OBJECTDIR)byteswapfns.oXVERSION = XV11R4XFLAGS = -DXWINDOW -D$(XVERSION)# This is to make the %$#@! Apollo cc happyOEXT = .o# OPTFLAGS is normally -O2.OPTFLAGS = -O  -fcombine-regs -finline-functions -fstrength-reduce -fdelayed-branchDISPOPTFLAGS = -O -fcombine-regs -finline-functions -fstrength-reduce -fdelayed-branchFPFLAGS =DFLAGS = -DFSERROR -DNEW_STORAGE -DAIX -DOLD_CURSOR -DNOPIXRECT -DBYTESWAP \	-DFORKCOMM -DLOGINT -DSYS5 -DDEC3100 -DNOETHER $(XFLAGS)  LDFLAGS = -lX11 -lc -lm# -Dsparc?INLINE = OBJECTDIR = ../$(RELEASENAME)/default	: ../$(OSARCHNAME)/lde ../$(OSARCHNAME)/ldeether# Special rules to create xc.c on Sun4#run cpp to expand macros$(OBJECTDIR)xc.o:	$(SRCDIR)xc.c $(INCDIR)lispemul.h  $(INCDIR)emulglobal.h  $(INCDIR)address.h \	 $(INCDIR)address68k.h  $(INCDIR)stack.h  $(INCDIR)lispglobal.h \	 $(INCDIR)lisptypes.h  $(INCDIR)lispmap.h  $(INCDIR)cell.h \	 $(INCDIR)initatoms.h  $(INCDIR)gc.h  \	 $(INCDIR)arith.h $(INCDIR)stream.h \	 $(INCDIR)tos1defs.h  $(INCDIR)tosretmacro.h \	 $(INCDIR)tosfuncall.h  $(INCDIR)inlineC.h   \	 $(INCDIR)inline68K.h	$(CC) -c   $(DFLAGS) -I$(INCDIR) $(SRCDIR)xc.c -o  $(OBJECTDIR)xc.o