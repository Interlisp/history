INSTALLDIR = /usr/local/ldeTARDEV = /dev/rst0TARFLAGS = xvfTAR = tarall: medley rooms loops notecardsrooms: lde medley	cd $(INSATLLDIR);$(TAR) $(TARFLAGS) $(TARDEV) $@loops: lde medley	cd $(INSATLLDIR);$(TAR) $(TARFLAGS) $(TARDEV) $@notecards: lde	cd $(INSATLLDIR);$(TAR) $(TARFLAGS) $(TARDEV) $@medley: lde	cd $(INSATLLDIR);$(TAR) $(TARFLAGS) $(TARDEV) $@lde:	if [test -d $(INSTALLDIR)]\	then echo "I refuse to write over your old $(INSTALLDIR)"; exit\	fi