#!/bin/csh -f## This shell script updates the archive file# /ftp/pcl/archive/cloops.text.## % update-file cloops[version-number].text# This copies cloops.text to cloops[version-number].text# and make cloops.text empty.# Yasuhiko Kiuchi(kiuchi.pa@xerox.com)#umask 0002set ArchiveDirectory = /ftp/pcl/archiveset ArchiveFile = /ftp/pcl/archive/cloops.textset DestinationFile = $*cd ${ArchiveDirectory}if (-e  ${DestinationFile}) then  echo "Error: file $DestinationFile already exists."else  cp ${ArchiveFile} ${DestinationFile}  cp /dev/null ${ArchiveFile}endif