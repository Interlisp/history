 :TYPES '((MAPSEQUENCE (:SEQUENCE                                                                           (:STRUCT MAPSEQUENCE                                                                                  (PROGRAM :UNSIGNED)                                                                                  (VERS :UNSIGNED)                                                                                  (PROTOCOL :UNSIGNED                                                                                         )                                                                                  (PORT :UNSIGNED))))                                                             )          :PROCEDURES          '((NULL 0 NIL NIL)            (LOOKUP 3 (:UNSIGNED :UNSIGNED :UNSIGNED :UNSIGNED)                   (:UNSIGNED))            (DUMP 4 NIL (MAPSEQUENCE))            (INDIRECT 5 (:UNSIGNED :UNSIGNED :UNSIGNED :STRING)                   (:UNSIGNED :STRING))))   (IL:* IL:|;;| "TCP version of same.  Sad that we need this redundancy.")   (DEFINE-REMOTE-PROGRAM 'TCPPORTMAPPER 100000 2 'TCP :TYPES          '((MAPSEQUENCE (:SEQUENCE (:STRUCT MAPSEQUENCE (PROGRAM :UNSIGNED)                                           (VERS :UNSIGNED)                                           (PROTOCOL :UNSIGNED)                                           (PORT :UNSIGNED)))))          :PROCEDURES          '((NULL 0 NIL NIL)            (LOOKUP 3 (:UNSIGNED :UNSIGNED :UNSIGNED :UNSIGNED)                   (:UNSIGNED))            (DUMP 4 NIL (MAPSEQUENCE))            (INDIRECT 5 (:UNSIGNED :UNSIGNED :UNSIGNED :STRING)                   (:UNSIGNED :STRING)))))(EVAL-WHEN (LOAD)(PORTMAPPERDEF))(IL:PUTPROPS IL:RPCPORTMAPPER IL:COPYRIGHT ("Stanford University and Xerox Corporation" 1987 1988 1990))(IL:DECLARE\: IL:DONTCOPY  (IL:FILEMAP (NIL)))IL:STOP