he implementation of INSTANCES as a file package           type.)    (SELECTQ type        (INSTANCES (LET ((instances (if (EQ '* (CADR com))                                        then (EVAL (CADDR com))                                      else (CDR com))))                        (SELECTQ name                            ((NIL T)                                  instances)                            (COND                               ((LITATOM name)                                (FMEMB name instances))                               ((UIDP name)                                (for i in instances when (UIDP i) thereis (UIDEqual name i)))                               (T (for n in name when (for i in instances when (UIDP i)                                                         thereis (UIDEqual name i)) collect n))))))        NIL)))(DEFINST  (NLAMBDA DEFINST% FORM                                     (* dgb%: " 4-OCT-83 10:07")                                                             (* Defining form for instances on a                                                              file)    (_ (OR (GetClassRec (CAR DEFINST% FORM))           (PROGN (printout T (CAR DEFINST% FORM)                         " has no class defined for it" T "Defining one now:" T)                  (_ ($ Class)                     New                     (CAR DEFINST% FORM))))       FileIn       (CDR DEFINST% FORM))))(DEFINSTANCES  (NLAMBDA Instances                                         (* dgb%: " 8-Oct-84 16:59")                    (* Read In the list of named instances, creating a new object with the named           UID. Make sure it has other names as given in the list.          This insures that other later references point to this same instance.)    (MAPC Instances (FUNCTION (LAMBDA (classNameInstNames)                                (PROG (instNames className UID obj)                                      (COND                                         ((NLISTP (SETQ className (CDR classNameInstNames)))                                                             (* Old format input.                                                             UID passed as NIL to NewObject)                                          (SETQ instNames (LIST (CAR classNameInstNames))))                                         (T (SETQ className (CAR classNameInstNames))                                            (SETQ instNames (REVERSE (CDR classNameInstNames)))                                            (SETQ UID (pop instNames))))                                      (COND                                         ((NULL className)                                          (RETURN)))                                      (NameObject (NewObject (GetObjectRec className)                                                         UID)                                             instNames)))))))(INSTANCESEditDef  (LAMBDA (name type source editComs)                        (* smL "24-Apr-86 18:19")                    (* * Edit the named instance)    (SELECTQ type        (INSTANCES (LET ((obj ($! name)))                        (if obj                            then (_ obj Edit editComs)                                 name                          else NIL)))        NIL)))(PrttyThese-Instances  (LAMBDA (instances)                                        (* smL "15-Aug-86 10:24")                    (* * Dump out these instances on PRTTYFILE.          Used by the FILEPKGCOM THESE-INSTANCES. Unlike the FILEPKGCOM INSTANCES, this           does not include any instances pointed to by these instances.)    (LET ((OutInstances))         (for objName in instances bind obj do (SETQ obj (if (Object? objName)                                                             then objName                                                           else ($! objName)))                                               (if obj                                                   then (PrettyPrintInstance obj (OUTPUT))                                                        (TERPRI)                                                        (TERPRI)                                                 else (HELPCHECK objName                                                              " not the name of an instance!"                                                              "Type OK to proceed.")))))))(PUTDEF (QUOTE INSTANCES) (QUOTE FILEPKGCOMS) '((COM MACRO [INSTANCES (E (PrttyInstances 'INSTANCES]                                                     CONTENTS ContainsOnlyInstances)                                                (TYPE DESCRIPTION "instances" GETDEF                                                       InstanceNotMethod WHENCHANGED (                                                                                    FnOrMethodChanged                                                                                     )                                                      EDITDEF INSTANCESEditDef)))(PUTDEF (QUOTE THESE-INSTANCES) (QUOTE FILEPKGCOMS) '((COM MACRO [INSTANCES                                                                  (E (PrttyThese-Instances                                                                      'INSTANCES]                                                           CONTENTS ContainsOnlyInstances)))(ADDTOVAR FILEPKGTYPES (INSTANCE . INSTANCES))(* ;;; "FilePackage type for methods")(DEF-DEFINE-TYPE METHOD-FNS "Method functions")(DEFINEQ(PARSE-METHOD-BODY  (LAMBDA (method-body)                                      (* ; "Edited  1-Oct-87 11:19 by smL")                    (* ;; "Parse the method def'n body, which should look like (Method {prop value}* ((class-name selector) ...args) ...forms), returning 6 values:  the class name, the selector, the arg list, a list of all declarations, the list of forms (less the declarations and doc string), the doc string, a p-list of qualifier-value pairs, and the method type (i.e. the CAR of the def'n).")    (LET (method-type arg-list class-name selector doc forms declarations qualifier-plist)                    (* ;; "Get the method type")         (SETQ method-type (pop method-body))                    (* ;; "Collect the qualifier p-list")         (while (AND (NOT (NULL (CAR method-body)))                     (LITATOM (CAR method-body))) do (SETQ qualifier-plist                                                      `(,.qualifier-plist ,(pop method-body)                                                              ,(pop method-body))))                    (* ;; "Get the arg-list, class-name, and selector")         (SETQ arg-list (pop method-body))         (SETQ class-name (CAAR arg-list))         (SETQ selector (CADAR arg-list))         (pop arg-list)         (CL:CHECK-TYPE class-name CL:SYMBOL)         (CL:CHECK-TYPE selector CL:SYMBOL)         (CL:CHECK-TYPE arg-list LIST)                    (* ;; "Now process the forms, collecting declarations and a possible doc-string")         (SETQ forms method-body)         (CL:LOOP (if (NULL (CDR forms))                      then                                   (* ;                                     "Last form in the list, so can't be a declaration or the comment")                           (RETURN)                    elseif (LITATOM (CAR forms))                      then                                   (* ;                                                            "Well, it doesn't lead off with a comment")                           (RETURN)                    elseif (STRINGP (CAR forms))                      then                                   (* ; "Could be a documentation string")                           (if (NULL doc)                               then                          (* ; "The doc string")                                    (SETQ doc (pop forms))                             else                            (* ;                                       "Already have a doc string, so this must be a part of the body")                                  (RETURN))                    elseif (OR (EDITDATE? (CAR forms))                               (AND (EQ (CAR (CAR forms))                                        COMMENTFLG)                                    (MEMB (CADR (CAR forms))                                          '(; ;; ;;;))                                    (STRINGP (CADDR (CAR forms)))                                    (STRPOS " " (CADDR (CAR forms)))                                    (IDATE (SUBSTRING (CADDR (CAR forms))                                                  (STRPOS " " (CADDR (CAR forms)))))))                      then                                   (* ; "Skip over the edit date")                           (pop forms)                    elseif (OR (EQ (CAR (CAR forms))                                   'DECLARE%:)                               (EQ (CAR (CAR forms))                                   'DECLARE))                      then                                   (* ; "A declaration")                           (SETQ declarations `(,.declarations ,(pop forms)))                    elseif (EQ (CAR (CAR forms))                               COMMENTFLG)                      then                                   (* ; "A leading comment")                           (LET ((comment (pop forms)))                                (if doc                                    then                     (* ;                                                    "Already got a doc-string, so ignore this comment")                                         NIL                                  elseif (AND (MEMB (CADR comment)                                                    '(; ;; ;;;))                                              (STRINGP (CADDR comment)))                                    then                     (* ; "A new-style comment")                                         (SETQ doc (CADDR comment))                                  elseif (EQ (CADR comment)                                             COMMENTFLG)                                    then                     (* ;                                                              "An old-style comment like (* * ...)")                                         (SETQ doc (SUBSTRING (CL:FORMAT NIL "窿媚囊泔眄孱舂博屐箦í⒘镬洵篝戾泔眄孱扉脲íㄓ旁滹ㄓ章釉疑吻锰浩弦土紊窿媚泔眄孱舂博┅屐箦í⒘蝈犰骘蝽箫孱翳箦狎汨骘盹蝈溴沆狎狒轱铙ㄒ旁找惟┅锰褐撂张沆狍蟓钺礤箦戾泗矧狎绛扉篝溴沆狎狒轱铙骘蝽滹聃犰殒殄颦痨轶礤翳镤豉疱┅┅ㄐ撩谁团匀夏孪馁ㄌ镣履ㄣ灬篌钺礤箦戾泗矧狎绛扉篝溴沆狎狒轱铙骘蝽滹聃犰殒殄蝮礤翳镤豉疱í⑴溟翦杯香舡阜肮喊怡箜挞啜ㄏ礤翳镤豉疱湾翳镤礼踽扉骈弪è沆狍蟓钺礤箦戾泗矧泪蜱扉篝括殒滹翳孱ㄌ捎滹悌屐箦紊泰冷邈灬蜥糸镱梨矧眢┅晌釉撂汰团匀夏莆厶镣履ㄣ灬篌钺礤箦戾泗矧骖钺礤狎珞滹悌í⑴溟翦脖术瞽父狈翰怡粤挞ㄌ旁è沆狍à沆狍蟓钺礤┅礤翳镶ㄇ弭湾翳镤镶沆狍箦戾泗矧冤麒弪彘竺灬篌弩龄渫弭栾沆狍箦戾泗矧骖钺礤汨犷珏礤翳镶礤翳镤骖钺礤汨犷珏礤翳镶狎珞狎珞汨犷珏礤翳镶滹悌滹悌孟文è蜗芴镝溟铉崎戾┅ㄓ旁塘釉紫夷骖钺礤í换⒃撂义盹鲥孟文è廖ㄎ仗ㄗ扰遗捎骖钺礤团匀夏萤ㄓ旁麒弪彘竺灬篌弩ㄗ扰遗捎沆狍蟓钺礤锰劣优萤┅聊脑掀商骖钺礤团匀夏昧麒弪彘竺灬篌弩┅┅┅í换⑹衣义盹鲥ㄍ烈肆用攘吻拍骖钺礤团匀夏呐粕闻末翳轭轭翳黠蜢镦团匀夏莆翳轶磲螂轭轶铒铄邃邃翳轶南庞黠蝌礤翳秕玷┊ㄕ瓮烈肆用攘吻拍骖钺礤莆萤ㄕ瓮烈肆用攘吻拍骖钺礤晌釉廖门萤骖钺礤荸湾翳镤祁蟓就弭栾潴厶镣履钺礤豉疱蝈狍镱í⑴溟翦淡琉颦父卑罕怡忉铄í换⑹衣絮铘铒骝镯磲螂轭团匀夏莆狍汨犷珏洚族箬秕熹栳鲥滹铄弼弪翳轭屐箦箝珙殒殂犷怡铒箝钽翳轶骢钽糸镱轶狒翳孱镦土宜劣萌廖桥钠斡ㄡ戾狍轸栳忮趑弪忮々殒ㄅ豉疱团匀夏莆萤翳孱ㄍ烈肆用攘吻拍钺礤团匀夏蝈狍镱ㄒ旁埔贤土宜劣萌廖桥妮呐颇牌晌乓湾翳镤酆瘟团锰禾镣履礤翳镤怙澌锰和仗陨刑怒至陶怒律文ㄣ灬篌钺礤箦戾泗矧辛矣怒团匀夏孪馁礤翳镤怙澌ㄍ弭栉犴沆狍蟓钺礤箦戾泗矧荸团匀夏莆é兹咸礤翳镤怙澌勖毯驼淘尚膛至陶怒律文ㄣ灬篌钺礤箦戾泗矧狎珞溴沆狎狒轱铙骘蝽滹聃犰殒殄蝮礤翳镤豉疱辛矣怒团匀夏孪馁礤翳镤怙澌锰毫佑乓渺狍罂à沆狍蟓钺礤┅ㄌ旁ㄦ躅泗轱瞽钺礤骢钽糸镱豉疱怙澌í换⒚镯瘐翦翳钺礤镦翳骢钽糸镱ㄓ旁骢钽糸镱钺礤ㄍ弭栉犴沆狍蟓钺礤箦戾泗矧┅í换⒚镯瘐翦翳豉疱镦翳骢钽糸镱ㄓ旁骢钽糸镱豉疱ㄏㄌ捎郧旁聃犰殒殄蝮浩瘴迷上苇再信荷泰锰好扰盟再信骢钽糸镱豉疱锰和磐屡好荷泰í换⑶弭翳怙澌镦翳骢钽糸镱鏖翳翳麸戾鲥泔眄孱趔蝈盹鲥洧塾旁怙澌啜锰好贤猩膛噎膛郇硫珞湘湾翳镤洛轭缑镯痖戾洫К狎珞í渺狍笪犴逑嫱弭栾湎黝弪К沆狍蟓钺礤í渝戾泗矧湘湾翳镤洛轭缑镯痖戾洫К箦戾泗矧í渝戽湘湾翳镤洛轭缑镯痖戾洫К昧狎珞骘蝽筝í换⒙蹰熹翳骢钽糸镱溴骈铋糸镱骘蝽啜幸锨锰号昧优骢钽糸镱豉疱ê锰啜锰耗牌瘴骢钽糸镱钺礤狎珞冷邈灬蜥糸镱怙澌┅ê商啜呐粕闻郜骢钽糸镱钺礤ㄌ镣履狎珞冷邈灬蜥糸镱怙澌荸┅晌釉撂汰团匀夏莆К沆狍蟓钺礤К箦戾泗矧К骢钽糸镱钺礤К媚狎珞К滹悌К骢钽糸镱钺礤荸ㄐ赵幸闲湾翳镤烈俏镣庞ǎ塄＼锰劣迎瘟团优膛迷弦＼优唐＼至＼＼＼葡彝＼┅í换⑼徙蝻骘蝽弪禊溴骈铄怡土靡咸旁轭箝溴湾翳镤ㄡ怙鲥┊物滹铄狍箦疳蜥翦磲泸矬骘疱蜴矧磲钽蝈狍镱螽誓英呐仆撩蚁哂躔弪é遗釉渝钿吁疱颦硫珞呐锰烈锰河信蒙撂硫珞湘湾翳镤洛轭缑镯痖戾洫渺狍笪犴逑嫱弭栾湎黝弪渝戾泗矧湘湾翳镤洛轭缑镯痖戾洫渝戽湘湾翳镤洛轭缑镯痖戾洫┅勖衔郇握烫渝钿吁疱颦硫珞í⒘蜱溴驷蹯麸狎珞镦翳礤翳镤啜滔闲迎普蚊撂燮轭溆躔弪湾翳镤渝戽湘湾翳镤洛轭缑镯痖戾洫渝戾泗矧湘湾翳镤洛轭缑镯痖戾洫ㄌ狭脑赏琶衔釉廖ㄏ熹渺狍渺狍笪犴逑嫱弭栾湎黝弪硫珞湘湾翳镤洛轭缑镯痖戾洫è闻渝戾泗矧湘湾翳镤洛轭缑镯痖戾洫昧囊渝钿吁疱颦硫珞┅í⒂屐邈麸蝮眭篝磲翥琚ㄅ乙弦⒂屐邈麸麸哂躔弪滹弩铒磲翥礤翳