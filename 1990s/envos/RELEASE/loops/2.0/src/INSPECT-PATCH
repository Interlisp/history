                        |of| frame)))                       (t (inspectcode atm)))                    |else|                    (let ((proc (and window (windowprop window 'process)))                          (editorargs (|if| (eq editor 'ed)                                            |then|                                            (list atm '(method-fns functions fns \:dontwait \:display                                                              ))                                            |else|                                            (list atm))))                         (|if| proc |then| (process.apply proc editor editorargs)                               |else|                               (cl:apply editor editorargs)))))))))(PUTPROPS INSPECT-PATCH COPYRIGHT ("Venue & Xerox Corporation" 1988 1990))(DECLARE\: DONTCOPY  (FILEMAP (NIL (487 2761 (INSPECT/AS/FUNCTION 497 . 2759)))))STOP