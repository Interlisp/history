     (RETURN (_Super self ComputeDisplayVal boundedReading))))(Method ((BoundedMixin OutOfBounds) self outFlg)             (* ; "edited: 27-Jan-87 11:37") "Print ? if out, space other wise" [LET* ((font (@ font))                                           (strWidth (STRINGWIDTH "??" font))                                           (leftPos (IDIFFERENCE (InteriorWidth self)                                                           strWidth))                                           (window (@ window)))                                          (COND                                             (outFlg (MOVETO leftPos 1 window)                                                    (PRIN1 "??" (@ window)))                                             (T (BITBLT NIL NIL NIL window leftPos 0 strWidth                                                       (FONTHEIGHT font)                                                       'TEXTURE                                                       'REPLACE WHITESHADE])(\UnbatchMethodDefs)(PUTPROPS GAUGEBOUNDEDMIXIN COPYRIGHT ("Xerox Corporation" 1986 1987 1988))(DECLARE%: DONTCOPY  (FILEMAP (NIL)))STOP